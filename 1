f  =  open ( 'task1.txt' , 'r' )   # наша база, открываем файл task1.txt, после чего, задаем необходимые взаимодействия
f  =  f . readlines ()
f  =  список ( map ( лямбда  x : str ( x ) [: - 1 ], f ))
f  = [ int ( x ) вместо  x  в  f ]
res  = []

для  x  в  f :
    для  y  в  f :
        для  z  в  f :
            если ( x  +  y  +  z ) ==  2020 :
                рез . добавить ( x  *  y  *  z )
res  =  список ( набор ( res ))
print ( "Наше число:" , * res )

# необходимо избавиться от дублированных чисел, полученных в результате перемножения одинаковых элементов
с  open ( 'task1_output.txt' , 'w' ) как  файл :
    для  x  в  разрешении :
        файл . написать ( str ( x ))
        файл . написать ( ' \ n ' )
файл . закрыть ()
